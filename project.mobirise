            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "*:focus": {
              "outline": "none"
            },
            ".form-group": {
              "margin-bottom": "2rem"
            },
            ".form-control": {
              "width": "100%",
              "border": "none",
              "background": "transparent",
              "border-bottom": "2px solid @inputColor",
              "color": "@inputColor",
              "border-radius": "0",
              "padding-left": "0",
              "padding-right": "0",
              "padding-bottom": "1px",
              "min-height": "35px",
              "&:focus": {
                "outline": "none",
                "box-shadow": "none"
              },
              "&::-webkit-input-placeholder": {
                "color": "@inputColor"
              },
              "&::-moz-placeholder": {
                "color": "@inputColor"
              },
              "&:-ms-input-placeholder": {
                "color": "@inputColor"
              },
              "&:-moz-placeholder": {
                "color": "@inputColor"
              }
            },
            "input.form-control": {
              "padding": "1px 0"
            },
            ".soc-item": {
              "display": "inline-block",
              "margin": "0 .2rem",
              ".socicon": {
                "font-size": "1.5rem",
                "color": "@iconsColor",
                "opacity": "1",
